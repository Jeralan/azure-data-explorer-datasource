{
  "name": "grafana-azure-data-explorer-datasource",
  "version": "2.0.1",
  "description": "Grafana data source for Azure Data Explorer",
  "scripts": {
    "build": "grafana-toolkit plugin:build",
    "test": "grafana-toolkit plugin:test",
    "dev": "grafana-toolkit plugin:dev",
    "watch": "grafana-toolkit plugin:dev --watch"
  },
  "author": "Grafana Labs",
  "license": "Apache",
  "dependencies": {
    "moment": "2.24.0",
    "monaco-editor": "0.17.0"
  },
  "devDependencies": {
    "@grafana/toolkit": "next",
    "@types/angular": "1.6.49",
    "@types/grafana": "github:CorpGlory/types-grafana.git"
  },
  "jest": {
    "moduleNameMapper": {
      "app/core/utils/kbn": "<rootDir>/src/__mocks__/kbn.ts",
      "app/plugins/sdk": "<rootDir>/src/__mocks__/sdk.ts",
      "monaco-editor": "<rootDir>/node_modules/monaco-editor/monaco.d.ts"
    }
  }
}
